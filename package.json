{
  "name": "guid",
  "displayName": "guid",
  "description": "A command that creates a new guid.",
  "version": "0.0.2",
  "publisher": "kosunix",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.guid"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.guid",
        "title": "Generate GUID"
      },
      {
        "command": "extension.guid_lowercase",
        "title": "Generate GUID (lowercase)"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+shift+[",
        "command": "extension.guid"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "typescript": "^5.1.3",
    "@types/vscode": "^1.73.0",
    "@vscode/test-electron": "^2.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "mocha": "^9.2.2"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  }
}